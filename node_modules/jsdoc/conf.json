{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/inferModule"],
    "inferModule": {
        "exclude": [
            "lib/foo/b.js"
        ],
        "schema": [
            { "from": "^lib\\/foo\\/(.*)\\.js$", "to": "bar/$1" },
            { "from": "^lib\\/fin\\/(.*)\\.js$", "to": "baz/$1" },
            { "from": "^lib\\/(.*\\.js$)", "to": "$1"}
        ]},
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}
